{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/main",
      "dest": "/index.html"
    },
    {
      "src": "/main/home",
      "dest": "/index.html"
    },
    {
      "src": "/main/ofertas",
      "dest": "/index.html"
    },
    {
      "src": "/main/libro/:id",
      "dest": "/index.html"
    },
    {
      "src": "/main/checkout",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
