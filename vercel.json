{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/main",
      "dest": "/"
    },
    {
      "src": "/main/home",
      "dest": "/index.html"
    },
    {
      "src": "/main/ofertas",
      "dest": "/index.html"
    },
    {
      "src": "/main/libro/:id",
